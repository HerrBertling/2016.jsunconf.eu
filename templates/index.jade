  // Begin MailChimp Signup Form
  #mc_embed_signup
    form#mc-embedded-subscribe-form.validate.stay-up-to-date(action='//jsunconf.us11.list-manage.com/subscribe/post?u=562adf61069508a950eb241c9&id=f013091375', method='post', name='mc-embedded-subscribe-form', target='_blank', novalidate='')
      #mc_embed_signup_scroll
        .mc-field-group
          input#mce-EMAIL.required.email(type='email', value='', name='EMAIL', placeholder='your email address')
        label(for='mce-EMAIL') stay tuned for updates
        #mce-responses.clear
          #mce-error-response.response(style='display:none')
          #mce-success-response.response(style='display:none')
        // real people should not fill this in and expect good things - do not remove this or risk form bot signups
        div(style='position: absolute; left: -5000px;')
          input(type='text', name='b_562adf61069508a950eb241c9_f013091375', tabindex='-1', value='')
        .clear
          button#mc-embedded-subscribe.button(type='submit', name='subscribe') ok
